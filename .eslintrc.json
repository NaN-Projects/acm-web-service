 {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "tsconfig.json",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "extends": ["airbnb"],
    "root": true,
    "env": {
        "node": true,
        "jest": true
    },
    "rules": {
        "import/no-unresolved": 0,
        "react/jsx-filename-extension": {
            "extensions": [
                ".jsx",
                ".tsx"
            ]
    }
}